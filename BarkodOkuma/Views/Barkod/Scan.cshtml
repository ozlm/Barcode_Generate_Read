@model IEnumerable<BarkodOkuma.Data.find_Result>

@{
    ViewBag.Title = "Scan";
}


<!DOCTYPE html>
<html>

<head>
    <title>Barkod</title>
    <meta name="description" content="website description" />
    <meta name="keywords" content="website keywords, website keywords" />

    <link href="~/bootstrap-4.1.1-dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/bootstrap-4.1.1-dist/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/bootstrap-4.1.1-dist/js/bootstrap.js"></script>
    <script src="~/bootstrap-4.1.1-dist/js/bootstrap.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <link rel="stylesheet" media="screen and (min-device-width: 1440px)" href="~/css/desktop.css" />

    <link rel='stylesheet' media='screen and (min-width: 800px) and (max-width: 1440px)' href='~/css/medium-style.css' />

    <link rel='stylesheet' media='screen and (min-width: 100px) and (max-width: 799px)' href='~/css/mobile-style.css' />



</head>
<body>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ENVANTER)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.STOKKODU)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.SATISFIYATI1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SATISFIYATI2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SATISFIYATI3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ALISFIYATI)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ENVANTER)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.STOKKODU)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.SATISFIYATI1)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SATISFIYATI2)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SATISFIYATI3)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ALISFIYATI)
                </td>

            </tr>
        }

    </table>
</body>
</html>